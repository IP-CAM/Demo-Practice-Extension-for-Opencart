<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Open Cart Test Suite" verbose="4" thread-count="2"
	parallel="tests">

	<listeners>
		<listener
			class-name="com.qa.opencartdemo.listeners.ExtentReportListener" />
		<listener
			class-name="com.qa.opencartdemo.listeners.TestAllureListener" />
	</listeners>


	<test name="Open Cart Login Page Test ">
		<parameter name="browser" value="chrome" />
		<parameter name="browserversion" value="89" />
		<classes>
			<class name="com.qa.opencartdemo.tests.LoginPageTest" />
		</classes>
	</test>

	<!-- <test name="Open Cart Account Page Test "> <classes> <class name="com.qa.opencart.tests.AccountsPageTest" 
		/> </classes> </test> <test name="Open Cart Registration Page Test "> <classes> 
		<class name="com.qa.opencart.tests.RegistrationPageTest" /> </classes> </test> -->

</suite>